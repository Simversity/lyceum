function addNew(evt_name, params){
    var newDiv = $('<div/>', {
	id:'div_' + evt_name,
	//class
    });
    
    var p1 = $("<p><span class = 'evtname'>" + evt_name + "</span><span class = 'evtfired'>"+params['fired']+"</span><span class = 'evtcompleted'>"+params['completed']+"</span></p>");
    p1.appendTo(newDiv);
    return newDiv
}

$.ajax({url:"http://localhost:8888/getlist/", type:'get', dataType:'json'}, function(e){
    events = JSON.parse(e);
    $.each(events, function(evt_name, params){
	nDiv = addNew(evt_name, params);
	nDiv.appendTo("#list")
	
    })
})

